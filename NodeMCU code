float PozY;
float PozZ;


float X1 = 80; //Naprijed
float X2 = 80;
float Y1 = 80; //Kuk u 8
float Y2 = 20; // Kuk u 45
float Z1 = -140;
float Z2 = -40;

float X1Z = 80;
float X2Z = 80;
